		"react": "^19.1.0",
		"react-dom": "^19.1.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@tailwindcss/postcss": "^4.1.8",
		"@types/node": "^22.15.29",
		"@types/react": "^19.1.6",
		"@types/react-dom": "^19.1.5",
		"tailwindcss": "^4.1.8",
		"typescript": "^5.8.3",
		"wrangler": "^4.45.3"
